    eprint       = 5280572,
    eprinttype   = {pmcid}
}
@article{2015-bmbed,
    title        = {Known Structure, Unknown Function: An Inquiry-Based Undergraduate Biochemistry Laboratory Course},
    author       = {Gray, Cynthia and Price, Carol W and Lee, Christopher T and Dewald, Alison H and Cline, Matthew A and McAnany, Charles E and Columbus, Linda and Mura, Cameron},
