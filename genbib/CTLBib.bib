@article{2014-msm-jctc,
    title        = {{Application of Molecular-Dynamics Based Markov State Models to Functional Proteins}},
    author       = {Malmstrom, Robert D. and Lee, Christopher T. and Van Wart, Adam T. and Amaro, Rommie E.},
